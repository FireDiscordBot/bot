{
  "name": "firediscordbot",
  "version": "1.1.0",
  "description": "Fire is a Discord bot that hopes to be the only one you'll need!",
  "main": "dist/index",
  "repository": "https://github.com/FireDiscordBot/bot.git",
  "author": "GamingGeek <geek@gaminggeek.dev> (https://gaminggeek.dev/)",
  "contributors": [
    "The Aero Team <team@aero.bot> (https://aero.bot)"
  ],
  "engines": {
    "node": ">=24.13.0"
  },
  "scripts": {
    "dev": "pnpm compile && pnpm rundev",
    "rundev": "GIT_COMMIT=$(git rev-parse HEAD) GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) NODE_ENV=development node --enable-source-maps dist/src/index.js",
    "compile": "rm -rf dist/ && tsc",
    "start": "GIT_COMMIT=${GIT_COMMIT:-$(git rev-parse HEAD || echo unknown)} GIT_BRANCH=${GIT_BRANCH:-$(git rev-parse --abbrev-ref HEAD || echo unknown)} NODE_ENV=production node --enable-source-maps dist/src/index.js",
    "format": "prettier . --write"
  },
  "_moduleAliases": {
    "@fire/lib": "dist/lib",
    "@fire/src": "dist/src",
    "@fire/config": "dist/config",
    "@fire/i18n": "dist/languages"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@aero/sanitizer": "^1.3.2",
    "@sapphire/async-queue": "^1.5.5",
    "@sentry/node": "8.49.0",
    "centra": "^2.7.0",
    "chalk": "4.1.2",
    "chrono-node": "^2.9.0",
    "dayjs": "^1.11.19",
    "discord-akairo": "discord-akairo/discord-akairo",
    "discord-api-types": "^0.38.37",
    "discord.js": "FireDiscordBot/discord.js",
    "dotenv-extended": "^2.9.0",
    "emoji-regex": "^10.6.0",
    "form-data": "^4.0.5",
    "fuzzball": "^2.2.3",
    "i18next": "^25.8.0",
    "module-alias": "^2.2.3",
    "murmurhash-js": "^1.0.0",
    "nanoid": "3.3.8",
    "p-event": "4.2.0",
    "pidusage": "3.0.1",
    "prettier": "^3.8.1",
    "semaphore-async-await": "^1.5.1",
    "semver": "^7.7.3",
    "tinycolor2": "^1.6.0",
    "ts-postgres": "1.3.1",
    "typescript": "^5.9.3",
    "uuid": "10.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/centra": "^2.2.3",
    "@types/humanize-duration": "^3.27.4",
    "@types/murmurhash-js": "^1.0.6",
    "@types/node": "^25.0.10",
    "@types/pidusage": "^2.0.5",
    "@types/semver": "^7.7.1",
    "@types/tinycolor2": "^1.4.6",
    "@types/uuid": "10.0.0",
    "@types/ws": "^8.18.1"
  }
}
