{
  "name": "firediscordbot",
  "version": "1.1.0",
  "description": "Fire is a Discord bot that hopes to be the only one you'll need!",
  "main": "dist/index",
  "repository": "https://github.com/FireDiscordBot/bot.git",
  "author": "GamingGeek <geek@gaminggeek.dev> (https://gaminggeek.dev/)",
  "contributors": [
    "The Aero Team <team@aero.bot> (https://aero.bot)"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "yarn compile && yarn rundev",
    "litecord": "yarn compile && yarn runlc",
    "rundev": "NODE_ENV=development node dist/src/index.js",
    "runlc": "NODE_ENV=litecord node dist/src/index.js",
    "compile": "rm -rf dist/ && tsc",
    "start": "NODE_ENV=production node dist/src/index.js",
    "format": "prettier . --write"
  },
  "_moduleAliases": {
    "@fire/lib": "dist/lib",
    "@fire/src": "dist/src",
    "@fire/config": "dist/config",
    "@fire/i18n": "dist/languages"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@aero/ksoft": "^3.1.8",
    "@aero/sanitizer": "^1.2.0",
    "@sapphire/async-queue": "^1.1.7",
    "@sentry/node": "^6.13.3",
    "@skyra/timestamp": "^1.0.0",
    "archiver": "^5.3.0",
    "centra": "^2.5.0",
    "chrono-node": "^2.3.2",
    "discord-akairo": "discord-akairo/discord-akairo",
    "discord-api-types": "^0.24.0",
    "discord.js": "FireDiscordBot/discord.js",
    "dotenv-extended": "^2.9.0",
    "emoji-regex": "^10.0.0",
    "fuzzball": "^2.0.2",
    "humanize-duration": "^3.27.0",
    "i18next": "^21.3.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "murmurhash-js": "^1.0.0",
    "nanoid": "^3.1.30",
    "node-color-log": "^7.0.0",
    "nodejs-assistant": "^0.8.0",
    "p-event": "^4.2.0",
    "pidusage": "^2.0.21",
    "semaphore-async-await": "^1.5.1",
    "source-map-support": "^0.5.20",
    "tinycolor2": "^1.4.2",
    "ts-postgres": "^1.1.3",
    "uuid": "^8.3.2",
    "ws": "^8.2.3",
    "zlib-sync": "^0.1.7"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.5",
    "utf-8-validate": "^5.0.7"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/centra": "^2.2.0",
    "@types/humanize-duration": "^3.25.1",
    "@types/murmurhash-js": "^1.0.3",
    "@types/node": "^16.11.1",
    "@types/pidusage": "^2.0.1",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.2.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
