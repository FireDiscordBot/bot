{
  "name": "firediscordbot",
  "version": "1.1.0",
  "description": "Fire is a Discord bot that hopes to be the only one you'll need!",
  "main": "dist/index",
  "repository": "https://github.com/FireDiscordBot/bot.git",
  "author": "GamingGeek <geek@gaminggeek.dev> (https://gaminggeek.dev/)",
  "contributors": [
    "The Aero Team <team@aero.bot> (https://aero.bot)"
  ],
  "engines": {
    "node": ">=16.20.2"
  },
  "scripts": {
    "fire:install": "yarn && yarn add discord-akairo/discord-akairo FireDiscordBot/discord.js",
    "dev": "yarn compile && yarn rundev",
    "rundev": "NODE_ENV=development node dist/src/index.js",
    "compile": "rm -rf dist/ && tsc && git rev-parse HEAD > dist/commit.txt",
    "start": "NODE_ENV=production node dist/src/index.js",
    "format": "prettier . --write"
  },
  "_moduleAliases": {
    "@fire/lib": "dist/lib",
    "@fire/src": "dist/src",
    "@fire/config": "dist/config",
    "@fire/i18n": "dist/languages"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@aero/sanitizer": "^1.3.2",
    "@sapphire/async-queue": "^1.5.5",
    "@sentry/node": "^10.23.0",
    "centra": "^2.7.0",
    "chalk": "4.1.2",
    "chrono-node": "^2.9.0",
    "dayjs": "^1.11.19",
    "discord-akairo": "discord-akairo/discord-akairo",
    "discord-api-types": "^0.37.120",
    "discord.js": "FireDiscordBot/discord.js",
    "dotenv-extended": "^2.9.0",
    "emoji-regex": "^10.6.0",
    "form-data": "^4.0.4",
    "fuzzball": "^2.2.3",
    "i18next": "^25.6.0",
    "module-alias": "^2.2.3",
    "murmurhash-js": "^1.0.0",
    "nanoid": "5.1.6",
    "p-event": "^7.0.0",
    "pidusage": "^4.0.1",
    "prettier": "^3.6.2",
    "semaphore-async-await": "^1.5.1",
    "semver": "^7.7.3",
    "source-map-support": "^0.5.21",
    "tinycolor2": "^1.6.0",
    "ts-postgres": "1.3.1",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/centra": "^2.2.3",
    "@types/humanize-duration": "^3.27.4",
    "@types/murmurhash-js": "^1.0.6",
    "@types/node": "^16.18.126",
    "@types/pidusage": "^2.0.5",
    "@types/semver": "^7.7.1",
    "@types/tinycolor2": "^1.4.6",
    "@types/ws": "^8.18.1"
  },
  "volta": {
    "node": "16.13.2"
  }
}
